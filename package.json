{
  "name": "fullfeeds",
  "version": "0.0.1",
  "description": "Enhances RSS feeds by following their links, extracting additional additional content from those pages and incorporating it into a new feed.",
  "author": "andrew morton <drewish@katherinehouse.com> (http://drewish.com/)",
  "dependencies": {
    "async": ">= 0.1.15",
    "feedparser": ">= 0.4.5",
    // Need to sort out an issue with newer versions of JSDOM
    "jsdom": "= 0.2.11",
    "redis": ">= 0.7.1",
    "rss": ">= 0.0.3",
    "request": ">= 2.9.x",
    // Not sure I need to hard code this yet.
    "express": "2.5.8",
    "jade": ">= 0.0.1",
    "connect-redis": ">= 1.2.0"
  },
  "main": "./lib/fullfeeds",
  "engines": [
    "node"
  ],
  "directories": {
