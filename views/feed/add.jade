form#form-feed-add.form-horizontal(method='post', action='/feeds/new')
  fieldset
    legend Add
    div.control-group
      lable.control-label(for='url') URL
      div.controls
        input(type='text', name='feed[url]', id='url', value=feed.url)
    div.control-group
      lable.control-label(for='selector') Selector
      div.controls
        input(type='text', name='feed[selector]', id='selector', value=feed.selector)
    div.form-actions
      input.btn.btn-primary(type='submit', value='Save')
      &nbsp;
      a.btn(href='/feeds') Cancel
